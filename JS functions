<!-- REUSABLE FUNCTIONS-->
function reusableFunction(){
  console.log("Hi World");
}

reusableFunction();

<!-- Functions ith arguments-->
function functionWithArgs(param1,param2){
  console.log(param1+param2);
}

functionWithArgs(2,10)

<!--Local vs Global: if no var or var outsid eof function teh variable is global. inside function you need to write var then is local but if oyu forget is global-->
<!-- Local vars take precedence over global. -->

<!-- return-->
