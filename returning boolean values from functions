<!-- dont use this-->
function isLess(a, b) {
  if (a < b) {
    return true;
  } else {
    return false;
  }
}
<!USE THIS INSTEAD-->
function isLess(a, b) {
    return a<b;
}
<!--MUCH EASIER-->

<!--RETURN EARLY PATTERN FOR FUCNTION-->

function abTest(a, b) {
  if (a<0||b<0){
   return undefined;
  }
  return Math.round(Math.pow(Math.sqrt(a) + Math.sqrt(b), 2));
}
