<!IF FUNCTION-->
function trueOrFalse(wasThatTrue) {
if (wasThatTrue){
  return "Yes, that was true";
}
return "No, that was false";
}

<!--EQUALITY OPERATOR with type conversion for exampel 3="3"-->
function testEqual(val) {
  if (val==12) { 
    return "Equal";
  }
  return "Not Equal";
}
<!-- END-->

<!-- strict equality operator with no type conversion for example "3" is NOT=3-->
function testStrict(val) {
  if (val===12) {
    return "Equal";
  }
  return "Not Equal";
}
<!--END-->

function compareEquality(a, b) {
  if (a === b) { 
    return "Equal";
  }
  return "Not Equal";
}

function testNotEqual(val) {
  if (val!=99) { 
    return "Not Equal";
  }
  return "Equal";
}

function testStrictNotEqual(val) {
  if (val!==17) {
    return "Not Equal";
  }
  return "Equal";
}

